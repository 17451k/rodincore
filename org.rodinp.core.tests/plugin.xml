<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.core.runtime.contentTypes">
      <content-type
            base-type="org.rodinp.core.rodin"
            file-extensions="test"
            id="content-type-test"
            name="org.rodinp.core.tests.content-type-test"
            priority="normal"/>
      <content-type
            base-type="org.rodinp.core.rodin"
            file-extensions="ctx"
            id="context"
            name="Test context"
            priority="normal"/>
      <content-type
            base-type="org.rodinp.core.rodin"
            file-extensions="csc"
            id="scContext"
            name="test checked context"
            priority="normal"/>
      <content-type
            base-type="org.rodinp.core.rodin"
            file-extensions="po"
            id="poFile"
            name="test PO file"
            priority="normal"/>
      <content-type
            base-type="org.rodinp.core.rodin"
            file-extensions="mch"
            id="machine"
            name="Test machine"
            priority="normal"/>
      <content-type
            base-type="org.rodinp.core.rodin"
            file-extensions="msc"
            id="scMachine"
            name="test checked machine"
            priority="normal"/>
   </extension>
   <extension
         point="org.rodinp.core.fileElementTypes">
      <fileElementType
            class="org.rodinp.core.tests.basis.RodinTestFile"
            content-type-id="org.rodinp.core.tests.content-type-test"
            id="test"
            name="org.rodinp.core.tests.test"/>
      <fileElementType
            class="org.rodinp.core.tests.builder.basis.Context"
            content-type-id="org.rodinp.core.tests.context"
            id="context"
            name="Context"/>
      <fileElementType
            class="org.rodinp.core.tests.builder.basis.SCContext"
            content-type-id="org.rodinp.core.tests.scContext"
            id="scContext"
            name="Checked Context"/>
      <fileElementType
            class="org.rodinp.core.tests.builder.basis.POFile"
            content-type-id="org.rodinp.core.tests.poFile"
            id="poFile"
            name="PO File"/>
      <fileElementType
            class="org.rodinp.core.tests.builder.basis.Machine"
            content-type-id="org.rodinp.core.tests.machine"
            id="machine"
            name="Machine"/>
      <fileElementType
            class="org.rodinp.core.tests.builder.basis.SCMachine"
            content-type-id="org.rodinp.core.tests.scMachine"
            id="scMachine"
            name="Checked Machine"/>
   </extension>
   <extension
         point="org.rodinp.core.internalElementTypes">
      <internalElementType
            class="org.rodinp.core.tests.basis.NamedElement"
            id="namedElement"
            name="org.rodinp.core.tests.namedElement"/>
      <internalElementType
            class="org.rodinp.core.tests.builder.basis.Dependency"
            id="dependency"
            name="Dependency to another context"/>
      <internalElementType
            class="org.rodinp.core.tests.builder.basis.Data"
            id="data"
            name="Arbitrary data"/>
      <internalElementType
            class="org.rodinp.core.tests.builder.basis.Reference"
            id="reference"
            name="Reference to other machine"/>
   </extension>
   <extension
         point="org.rodinp.core.autoTools">
      <tool
            class="org.rodinp.core.tests.builder.CSCTool"
            id="testCSC"
            name="Dumb Context Static Checker">
         <extractor
               class="org.rodinp.core.tests.builder.CSCTool"
               id="testCSC"
               name="Dumb Context Static Checker Extractor">
            <inputType id="org.rodinp.core.tests.context"/>
         </extractor>
      </tool>
      <tool
            class="org.rodinp.core.tests.builder.MSCTool"
            id="testMSC"
            name="Dumb Machine Static Checker">
         <extractor
               class="org.rodinp.core.tests.builder.MSCTool"
               id="testMSC"
               name="Dumb Context Static Checker Extractor">
            <inputType id="org.rodinp.core.tests.machine"/>
         </extractor>
      </tool>
      <tool
            class="org.rodinp.core.tests.builder.CPOTool"
            id="testCPO"
            name="Dumb Context POG">
         <extractor
               class="org.rodinp.core.tests.builder.CPOTool"
               id="testCPO"
               name="Dumb Context POG Extractor">
            <inputType id="org.rodinp.core.tests.scContext"/>
         </extractor>
      </tool>
      <tool
            class="org.rodinp.core.tests.builder.MPOTool"
            id="testMPO"
            name="Dumb Machine POG">
         <extractor
               class="org.rodinp.core.tests.builder.MPOTool"
               id="testMPO"
               name="Dumb Machine POG Extractor">
            <inputType id="org.rodinp.core.tests.scMachine"/>
         </extractor>
      </tool>
   </extension>
   <extension
         point="org.rodinp.core.attributeTypes">
      <attributeType
          id="fBool"
          name="Boolean attribute"
          kind="boolean"/>
      <attributeType
          id="fHandle"
          name="Element handle attribute"
          kind="handle"/>
      <attributeType
          id="fInt"
          name="Integer attribute"
          kind="integer"/>
      <attributeType
          id="fString"
          name="String attribute"
          kind="string"/>
   </extension>

</plugin>
