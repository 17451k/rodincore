      <view
            category="org.eventb.ui"
            class="org.eventb.internal.ui.prover.ProofTreeUI"
            icon="icons/sample.gif"
            id="org.eventb.ui.views.ProofTreeUI"
            name="Proof Tree UI"/>
      <view
            category="org.eventb.ui"
            class="org.eventb.internal.ui.eventbeditor.InvariantMirror"
            icon="icons/sample.gif"
            id="org.eventb.ui.views.InvariantMirror"
            name="Invariant Mirror"/>
      <view
            category="org.eventb.ui"
            class="org.eventb.internal.ui.eventbeditor.TheoremMirror"
            icon="icons/sample.gif"
            id="org.eventb.ui.views.TheoremMirror"
            name="Theorem Mirror"/>
      <view
            category="org.eventb.ui"
            class="org.eventb.internal.ui.eventbeditor.AxiomMirror"
            icon="icons/sample.gif"
            id="org.eventb.ui.views.AxiomMirror"
            name="Axiom Mirror"/>
      <view
            category="org.eventb.ui"
            class="org.eventb.internal.ui.eventbeditor.CarrierSetMirror"
            icon="icons/sample.gif"
            id="org.eventb.ui.views.CarrierSetMirror"
            name="Carrier Set Mirror"/>
      <view
            category="org.eventb.ui"
            class="org.eventb.internal.ui.eventbeditor.ConstantMirror"
            icon="icons/sample.gif"
            id="org.eventb.ui.views.ConstantMirror"
            name="Constant Mirror"/>
      <view
            category="org.eventb.ui"
            class="org.eventb.internal.ui.eventbeditor.EventMirror"
            icon="icons/sample.gif"
            id="org.eventb.ui.views.EventMirror"
            name="Event Mirror"/>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.eventb.internal.ui.eventbeditor.EventBMachineEditor"
            contributorClass="org.eventb.internal.ui.eventbeditor.EventBMachineEditorContributor"
            extensions="bum"
            icon="icons/full/obj16/mch_obj.gif"
            id="org.eventb.ui.editors.EventBMachineEditor"
            name="Event-B Machine Editor"/>
      <editor
            class="org.eventb.internal.ui.eventbeditor.EventBContextEditor"
            contributorClass="org.eventb.internal.ui.eventbeditor.EventBContextEditorContributor"
            extensions="buc"
            icon="icons/full/obj16/ctx_obj.gif"
            id="org.eventb.ui.editors.EventBContextEditor"
            name="Event-B Context Editor"/>
      <editor
            class="org.eventb.internal.ui.prover.ProverUI"
            contributorClass="org.eventb.internal.ui.prover.ProverUIContributor"
            extensions="bpr"
            icon="icons/sample.gif"
            id="org.eventb.ui.editors.ProverUI"
            name="Prover User Interface"/>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.eventb.ui.wizards"
            name="Event-B Wizards"/>
      <wizard
            category="org.eventb.ui.wizards"
            class="org.eventb.internal.ui.wizards.NewProjectWizard"
            icon="icons/sample.gif"
            id="org.eventb.ui.wizards.NewProject"
            name="New Event-B Project"/>
      <wizard
            category="org.eventb.ui.wizards"
            class="org.eventb.internal.ui.wizards.NewComponentWizard"
            icon="icons/sample.gif"
            id="org.eventb.ui.wizards.NewComponent"
            name="New Event-B Component"/>
   </extension>
   <extension-point id="machinePages" name="Event-B Machine Form Page" schema="schema/machinePages.exsd"/>
   <extension-point id="contextPages" name="Event-B Context Form Page" schema="schema/contextPages.exsd"/>
   <extension-point id="globalProofTactics" name="Global Proof Tactic" schema="schema/globalProofTactics.exsd"/>
   <extension-point id="globalProofTacticDropdowns" name="Proof Tactic Dropdown" schema="schema/globalProofTacticDropdowns.exsd"/>
   <extension
         point="org.eventb.ui.machinePages">
      <page class="org.eventb.internal.ui.eventbeditor.DependenciesPage"/>
      <page class="org.eventb.internal.ui.eventbeditor.VariablePage"/>
      <page class="org.eventb.internal.ui.eventbeditor.InvariantPage"/>
      <page class="org.eventb.internal.ui.eventbeditor.TheoremPage"/>
      <page class="org.eventb.internal.ui.eventbeditor.EventPage"/>
      <page class="org.eventb.internal.ui.eventbeditor.SyntheticViewPage"/>
   </extension>
   <extension
         point="org.eventb.ui.contextPages">
      <page class="org.eventb.internal.ui.eventbeditor.DependenciesPage"/>
      <page class="org.eventb.internal.ui.eventbeditor.CarrierSetPage"/>
      <page class="org.eventb.internal.ui.eventbeditor.ConstantPage"/>
      <page class="org.eventb.internal.ui.eventbeditor.AxiomPage"/>
      <page class="org.eventb.internal.ui.eventbeditor.TheoremPage"/>
      <page class="org.eventb.internal.ui.eventbeditor.SyntheticViewPage"/>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.eventb.internal.ui.ModellingPerspective"
            id="org.eventb.ui.perspective.Modelling"
            name="Modelling"/>
      <perspective
            class="org.eventb.internal.ui.ProvingPerspective"
            id="org.eventb.ui.perspective.Proving"
            name="Proving"/>
   </extension>
   <extension
         point="org.eventb.ui.globalProofTactics">
      <tactic
            class="org.eventb.internal.ui.prover.globaltactics.Review"
            dropdown="reviews"
            icon="icons/full/ctool16/rv_prover.gif"
            id="org.eventb.ui.review"
            toolbar="sequent"
            tooltip="Review goal with selected hypotheses"/>
      <tactic
            class="org.eventb.internal.ui.prover.globaltactics.Normalise"
            icon="icons/full/ctool16/nm_prover.gif"
            id="org.eventb.ui.normalise"
            toolbar="sequent"
            tooltip="Normalise tactic"/>
      <tactic
            class="org.eventb.internal.ui.prover.globaltactics.Contradiction"
            icon="icons/full/ctool16/ct_prover.gif"
            id="org.eventb.ui.contradiction"
            toolbar="sequent"
            tooltip="Contradiction"/>
      <tactic
            class="org.eventb.internal.ui.prover.globaltactics.DoCase"
            icon="icons/full/ctool16/dc_prover.gif"
            id="org.eventb.ui.doCase"
            toolbar="sequent"
            tooltip="Case distinction"/>
      <tactic
            class="org.eventb.internal.ui.prover.globaltactics.AddHypothesis"
            icon="icons/full/ctool16/ah_prover.gif"
            id="org.eventb.ui.addHypothesis"
            toolbar="sequent"
            tooltip="Add hypothesis"/>
      <tactic
            class="org.eventb.internal.ui.prover.globaltactics.Backtrack"
            icon="icons/full/ctool16/bk_prover.gif"
            id="org.eventb.ui.backtrack"
            toolbar="backtrack"
            tooltip="Backtrack from the current node"/>
      <tactic
            class="org.eventb.internal.ui.prover.globaltactics.Prune"
            icon="icons/full/ctool16/pn_prover.gif"
            id="org.eventb.ui.prune"
            toolbar="backtrack"
            tooltip="Prune from the current node"/>
      <tactic
            class="org.eventb.internal.ui.prover.globaltactics.SearchHypotheses"
            icon="icons/full/ctool16/sh_prover.gif"
            id="org.eventb.ui.searchHypotheses"
            toolbar="navigation"
            tooltip="Search hypotheses"/>
      <tactic
            class="org.eventb.internal.ui.prover.globaltactics.Previous"
            icon="icons/full/ctool16/prev_prover.gif"
            id="org.eventb.ui.previous"
            toolbar="navigation"
            tooltip="Previous undischarged PO"/>
      <tactic
            class="org.eventb.internal.ui.prover.globaltactics.Next"
            icon="icons/full/ctool16/next_prover.gif"
            id="org.eventb.ui.next"
            toolbar="navigation"
            tooltip="Next undischarged PO"/>
      <tactic
            class="org.eventb.internal.ui.prover.globaltactics.P0"
            dropdown="externalProvers"
            id="org.eventb.ui.p0"
            icon="icons/full/ctool16/p0_prover.gif"
            tooltip="External prover: Predicate prover on selected hypotheses"/>
      <tactic
            class="org.eventb.internal.ui.prover.globaltactics.PP"
            dropdown="externalProvers"
            id="org.eventb.ui.pp"
            icon="icons/full/ctool16/pp_prover.gif"
            tooltip="External prover: Predicate prover on all hypotheses"/>
      <tactic
            class="org.eventb.internal.ui.prover.globaltactics.M0"
            dropdown="externalProvers"
            id="org.eventb.ui.m0"
            icon="icons/full/ctool16/m0_prover.gif"
            tooltip="External prover: Mono-Lemma prover in force 0"/>
      <tactic
            class="org.eventb.internal.ui.prover.globaltactics.M1"
            dropdown="externalProvers"
            id="org.eventb.ui.m1"
            icon="icons/full/ctool16/m1_prover.gif"
            tooltip="External prover: Mono-lemma prover in force 1"/>
      <tactic
            class="org.eventb.internal.ui.prover.globaltactics.M2"
            dropdown="externalProvers"
            id="org.eventb.ui.m2"
            icon="icons/full/ctool16/m2_prover.gif"
            tooltip="External prover: Mono-lemma prover in force 2"/>
      <tactic
            class="org.eventb.internal.ui.prover.globaltactics.M3"
            dropdown="externalProvers"
            id="org.eventb.ui.m3"
            icon="icons/full/ctool16/m3_prover.gif"
            tooltip="External prover: Mono-lemma prover in force 3"/>
      <tactic
            class="org.eventb.internal.ui.prover.globaltactics.ML"
            dropdown="externalProvers"
            id="org.eventb.ui.ml"
            icon="icons/full/ctool16/ml_prover.gif"
            tooltip="External prover: Mono-lemma prover in all force"/>
      <toolbar id="sequent"/>
      <toolbar id="backtrack"/>
      <toolbar id="navigation"/>
      <dropdown
            id="externalProvers"
            toolbar="sequent"/>
      <dropdown
            id="reviews"
            toolbar="sequent"/>
   </extension>
   <extension
         id="org.eventb.ui.eventbeditorActions"
         name="Event-B Editor Actions"
         point="org.eclipse.ui.editorActions">
      <editorContribution
            id="org.eventb.ui.machineContribution"
            targetID="org.eventb.ui.editors.EventBMachineEditor">
         <menu
               id="org.eventb.ui.eventBMenu"
               label="Event-B">
            <groupMarker name="New"/>
         </menu>
         <action
               actionID="org.eventb.ui.actions.newEvents"
               class="org.eventb.internal.ui.eventbeditor.actions.NewEvent"
               icon="icons/full/ctool16/newevt_edit.gif"
               id="org.eventb.ui.actions.newevent"
               label="New Event"
               menubarPath="org.eventb.ui.eventBMenu/New"
               style="push"
               toolbarPath="org.eventb.ui.eventBEditor"
               tooltip="Create a new event for the machine"/>
         <action
               actionID="org.eventb.ui.actions.newTheorems"
               class="org.eventb.internal.ui.eventbeditor.actions.NewTheorems"
               icon="icons/full/ctool16/newthm_edit.gif"
               id="org.eventb.ui.actions.newtheorems"
               label="New Theorems"
               menubarPath="org.eventb.ui.eventBMenu/New"
               style="push"
               toolbarPath="org.eventb.ui.eventBEditor"
               tooltip="Create new theorems for the machine"/>
         <action
               actionID="org.eventb.ui.actions.newVariable"
               class="org.eventb.internal.ui.eventbeditor.actions.NewVariable"
               icon="icons/full/ctool16/newvar_edit.gif"
               id="org.eventb.ui.actions.newvariable"
               label="New Variable"
               menubarPath="org.eventb.ui.eventBMenu/New"
               style="push"
               toolbarPath="org.eventb.ui.eventBEditor"
               tooltip="Create a new variable for the machine"/>
         <action
               actionID="org.eventb.ui.actions.newInvariants"
               class="org.eventb.internal.ui.eventbeditor.actions.NewInvariants"
               icon="icons/full/ctool16/newinv_edit.gif"
               id="org.eventb.ui.actions.newinvariants"
               label="New Invariants"
               menubarPath="org.eventb.ui.eventBMenu/New"
               style="push"
               toolbarPath="org.eventb.ui.eventBEditor"
               tooltip="Create new invariants for the machine"/>
         <action
               class="org.eventb.internal.ui.eventbeditor.actions.AutoInvNaming"
               id="org.eventb.ui.actions.autoInvNaming"
               label="Automatic Invariant Naming"
               menubarPath="org.eventb.ui.refactoring/InvName"
               style="push"/>
         <action
               class="org.eventb.internal.ui.eventbeditor.actions.PrefixInvName"
               id="org.eventb.ui.actions.PrefixInvNaming"
               label="Set Prefix Invariant Name"
               menubarPath="org.eventb.ui.refactoring/InvName"
               style="push"/>
         <action
               class="org.eventb.internal.ui.eventbeditor.actions.AutoThmNaming"
               id="org.eventb.ui.actions.autoThmNaming"
               label="Automatic Theorem Naming"
               menubarPath="org.eventb.ui.refactoring/ThmName"
               style="push"/>
         <action
               class="org.eventb.internal.ui.eventbeditor.actions.PrefixThmName"
               id="org.eventb.ui.actions.PrefixThmNaming"
               label="Set Prefix Theorem Name"
               menubarPath="org.eventb.ui.refactoring/ThmName"
               style="push"/>
         <menu
               id="org.eventb.ui.refactoring"
               label="Refactoring">
            <groupMarker name="InvName"/>
            <groupMarker name="ThmName"/>
         </menu>
      </editorContribution>
      <editorContribution
            id="org.eventb.ui.contextContribution"
            targetID="org.eventb.ui.editors.EventBContextEditor">
         <menu
               id="org.eventb.ui.eventBMenu"
               label="Event-B">
            <groupMarker name="New"/>
         </menu>
         <action
               actionID="org.eventb.ui.actions.newCarrierSets"
               class="org.eventb.internal.ui.eventbeditor.actions.NewCarrierSets"
               icon="icons/full/ctool16/newset_edit.gif"
               id="org.eventb.ui.actions.newcarriersets"
               label="New Carrier Sets"
               menubarPath="org.eventb.ui.eventBMenu/New"
               style="push"
               toolbarPath="org.eventb.ui.eventBEditor"
               tooltip="Create new carrier sets for the context"/>
         <action
               actionID="org.eventb.ui.actions.newConstants"
               class="org.eventb.internal.ui.eventbeditor.actions.NewConstants"
               icon="icons/full/ctool16/newcst_edit.gif"
               id="org.eventb.ui.actions.newconstants"
               label="New Constants"
               menubarPath="org.eventb.ui.eventBMenu/New"
               style="push"
               toolbarPath="org.eventb.ui.eventBEditor"
               tooltip="Create new contstants for the context"/>
         <action
               actionID="org.eventb.ui.actions.newAxioms"
               class="org.eventb.internal.ui.eventbeditor.actions.NewAxioms"
               icon="icons/full/ctool16/newaxm_edit.gif"
               id="org.eventb.ui.actions.newaxioms"
               label="New Axioms"
               menubarPath="org.eventb.ui.eventBMenu/New"
               style="push"
               toolbarPath="org.eventb.ui.eventBEditor"
               tooltip="Create new axioms for the context"/>
         <action
               actionID="org.eventb.ui.actions.newTheorems"
               class="org.eventb.internal.ui.eventbeditor.actions.NewTheorems"
               icon="icons/full/ctool16/newthm_edit.gif"
               id="org.eventb.ui.actions.newtheorems"
               label="New Theorems"
               menubarPath="org.eventb.ui.eventBMenu/New"
               style="push"
               toolbarPath="org.eventb.ui.eventBEditor"
               tooltip="Create new theorems for the context"/>
         <menu
               id="org.eventb.ui.refactoring"
               label="Refactoring">
            <groupMarker name="AxmName"/>
            <groupMarker name="ThmName"/>
         </menu>
         <action
               class="org.eventb.internal.ui.eventbeditor.actions.AutoThmNaming"
               id="org.eventb.ui.actions.autoThmNaming"
               label="Automatic Theorem Naming"
               menubarPath="org.eventb.ui.refactoring/ThmName"
               style="push"/>
         <action
               class="org.eventb.internal.ui.eventbeditor.actions.PrefixThmName"
               id="org.eventb.ui.actions.PrefixThmNaming"
               label="Set Prefix Theorem Name"
               menubarPath="org.eventb.ui.refactoring/ThmName"
               style="push"/>
         <action
               class="org.eventb.internal.ui.eventbeditor.actions.AutoAxmNaming"
               id="org.eventb.ui.actions.autoAxmNaming"
               label="Automatic Axiom Naming"
               menubarPath="org.eventb.ui.refactoring/AxmName"
               style="push"/>
         <action
               class="org.eventb.internal.ui.eventbeditor.actions.PrefixAxmName"
               id="org.eventb.ui.actions.PrefixAxmNaming"
               label="Set Prefix Axiom Name"
               menubarPath="org.eventb.ui.refactoring/AxmName"
               style="push"/>
      </editorContribution>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            description="Set of actions for Event-B Editor"
            id="org.eventb.ui.eventBEditor"
            label="Event-B"
            visible="false">
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            id="org.eventb.ui.MachineFile"
            objectClass="org.eventb.core.IMachineFile">
         <action
               class="org.eventb.internal.ui.projectexplorer.actions.Refines"
               enablesFor="1"
               icon="icons/full/ctool16/refines.gif"
               id="org.eventb.ui.refines"
               label="&amp;Refines"
               menubarPath="modelling"
               tooltip="Refine the machine"/>
         <action
               class="org.eventb.internal.ui.projectexplorer.actions.Proves"
               enablesFor="1"
               icon="icons/discharged.gif"
               id="org.eventb.ui.proveMachine"
               label="&amp;Proves"
               menubarPath="proving"
               tooltip="Prove the obligations related to this machine"/>
      </objectContribution>
      <objectContribution
            id="org.eventb.ui.ContextFile"
            objectClass="org.eventb.core.IContextFile">
         <action
               class="org.eventb.internal.ui.projectexplorer.actions.Extends"
               enablesFor="1"
               icon="icons/full/ctool16/refines.gif"
               id="org.eventb.ui.extends"
               label="&amp;Extends"
               menubarPath="modelling"
               tooltip="Extends the context"/>
         <action
               class="org.eventb.internal.ui.projectexplorer.actions.Proves"
               enablesFor="1"
               icon="icons/discharged.gif"
               id="org.eventb.ui.proveContext"
               label="&amp;Proves"
               menubarPath="proving"
               tooltip="Prove the obligations related to this context"/>
      </objectContribution>
      <viewerContribution
            id="org.eventb.ui.ProjectExplorer"
            targetID="org.eventb.ui.views.ProjectExplorer">
         <action
               class="org.eventb.internal.ui.projectexplorer.actions.Delete"
               enablesFor="+"
               icon="icons/delete.gif"
               id="org.eventb.ui.Delete"
               label="&amp;Delete"
               menubarPath="modelling"
               tooltip="Delete element(s)"/>
      </viewerContribution>
   </extension>

</plugin>
