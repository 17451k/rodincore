            </renameElement>
         </element>
      </simple>
      <simple
            type="org.rodinp.core.tests.versionFileB"
            version="2">
         <element
               elementPath="/org.rodinp.core.tests.versionFileB/org.rodinp.core.tests.versionEA">
            <renameAttribute
                  id="org.rodinp.core.tests.at"
                  newId="org.rodinp.core.tests.versionStrAttr">
            </renameAttribute>
         </element>
      </simple>
      <simple
            type="org.rodinp.core.tests.versionFileC"
            version="3">
         <element
               elementPath="/org.rodinp.core.tests.versionFileC/org.rodinp.core.tests.versionEA">
            <addAttribute
                  newId="org.rodinp.core.tests.versionStrAttr"
                  newValue="Hello">
            </addAttribute>
         </element>
      </simple>
