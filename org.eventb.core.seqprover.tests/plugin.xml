<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eventb.core.seqprover.reasoners">
      <reasoner
            class="org.eventb.core.seqprover.reasonerExtentionTests.TrueGoal"
            id="trueGoal"
            name="⊤ goal"/>
      <reasoner
            class="org.eventb.core.seqprover.reasonerExtentionTests.TrueGoal"
            id="erroneous.id"
            name="Erroneous id (contains a dot)"/>
      <reasoner
            class="org.eventb.core.seqprover.reasonerExtentionTests.TrueGoal"
            id="erroneous id"
            name="Erroneous id (contains a whitespace)"/>
      <reasoner
            class="org.eventb.core.seqprover.xprover.tests.TestReasoner"
            id="test"
            name="%reasoner.test.name"/>
      <reasoner
            class="org.eventb.core.seqprover.xprover.tests.ExtendedInputReasoner"
            id="extended"
            name="%reasoner.extended.name"/>
      <reasoner
            class="org.eventb.core.seqprover.rewriterTests.HideOriginalRewrites"
            id="hideOriginalRewrites"
            name="Simple rewriter (hide original)">
      </reasoner>
      <reasoner
            class="org.eventb.core.seqprover.rewriterTests.ShowOriginalRewrites"
            id="showOriginalRewrites"
            name="Simple rewriter (show original)">
      </reasoner>
      <reasoner
            class="org.eventb.core.seqprover.reasonerExtentionTests.ReasonerV1"
            id="reasonerV1"
            name="reasonerV1">
      </reasoner>
      <reasoner
            class="org.eventb.core.seqprover.proofBuilderTests.SuccessReasoner"
            id="successReasoner"
            name="Success Reasoner">
      </reasoner>
      <reasoner
            class="org.eventb.core.seqprover.proofBuilderTests.FailureReasoner"
            id="failureReasoner"
            name="Failure Reasoner">
      </reasoner>
   </extension>
   <extension
         point="org.eventb.core.seqprover.autoTactics">
      <autoTactic
            class="org.eventb.core.seqprover.autoTacticExtentionTests.IdentityTactic"
            description="This tactic does nothing but succeeds"
            id="identityTac"
            name="Identity Tactic"/>
      <autoTactic
            class="org.eventb.core.seqprover.autoTacticExtentionTests.IdentityTactic$FailTactic"
            id="failTac"
            name="Fail Tactic"/>
      <autoTactic
            class="org.eventb.core.seqprover.autoTacticExtentionTests.IdentityTactic"
            id="erroneous.id"
            name="Erroneous id (contains a dot)"/>
      <autoTactic
            class="org.eventb.core.seqprover.autoTacticExtentionTests.IdentityTactic"
            id="erroneous id"
            name="Erroneous id (contains a whitespace)"/>
      <autoTactic
            class="org.eventb.core.seqprover.autoTacticExtentionTests.ParameterizedTactics$TacWithParams"
            id="tacParam"
            name="Tactic with Parameters">
         <tacticParameter
               default="true"
               label="bool1"
               type="Boolean">
         </tacticParameter>
         <tacticParameter
               default="false"
               label="bool2"
               type="Boolean">
         </tacticParameter>
         <tacticParameter
               default="314"
               label="int1"
               type="Integer">
         </tacticParameter>
         <tacticParameter
               default="9223372036854775807"
               label="long1"
               type="Long">
         </tacticParameter>
         <tacticParameter
               default="formulæ"
               label="string1"
               type="String">
         </tacticParameter>
      </autoTactic>
      <autoTactic
            class="java.lang.Object"
            id="badInstance"
            name="Does not implement any required interface">
      </autoTactic>
      <autoTactic
            class="org.eventb.core.seqprover.autoTacticExtentionTests.ParameterizedTactics$Both"
            id="both"
            name="Implements Both ITactic and ITacticParameterizer">
      </autoTactic>
      <autoTactic
            class="org.eventb.core.seqprover.autoTacticExtentionTests.ParameterizedTactics$SimpleTacWithParam"
            id="tacWithParam"
            name="implements ITactic with parameter">
         <tacticParameter
               default="true"
               label="label"
               type="Boolean">
         </tacticParameter>
      </autoTactic>
      <autoTactic
            class="org.eventb.core.seqprover.autoTacticExtentionTests.ParameterizedTactics$ParamTacNoParam"
            id="noParam"
            name="Parameterizer without parameter">
      </autoTactic>
      <autoTactic
            class="org.eventb.core.seqprover.autoTacticExtentionTests.ParameterizedTactics$FakeTactic"
            id="notParseableInt"
            name="No Parseable Integer">
         <tacticParameter
               default="12.5"
               label="label"
               type="Integer">
         </tacticParameter>
      </autoTactic>
      <autoTactic
            class="org.eventb.core.seqprover.autoTacticExtentionTests.ParameterizedTactics$FakeTactic"
            id="notParseableLong"
            name="No Parseable Long">
         <tacticParameter
               default="9223372036854775807999"
               label="label"
               type="Long">
         </tacticParameter>
      </autoTactic>
      <autoTactic
            class="org.eventb.core.seqprover.autoTacticExtentionTests.ParameterizedTactics$FakeTactic"
            id="duplLabel"
            name="Duplicate Label">
         <tacticParameter
               default="true"
               label="dupl"
               type="Boolean">
         </tacticParameter>
         <tacticParameter
               default="true"
               label="dupl"
               type="Boolean">
         </tacticParameter>
      </autoTactic>
   </extension>
</plugin>
