<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         id="org.eclipse.ui.navigator.resourceContent"
         point="org.eclipse.ui.views">
      <category
            id="fr.systerel.explorer.category"
            name="%view.category.eventB">
      </category>
      <view
            category="fr.systerel.explorer.category"
            class="fr.systerel.explorer.navigator.RodinNavigator"
            icon="icons/full/clcl16/prj_expl.gif"
            id="fr.systerel.explorer.navigator.view"
            name="%view.navigator">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewer
            viewerId="fr.systerel.explorer.navigator.view">
         <options>
            <property
                  name="org.eclipse.ui.navigator.hideLinkWithEditorAction"
                  value="true">
            </property>
         </options>
      </viewer>
      <viewerContentBinding
            viewerId="fr.systerel.explorer.navigator.view">
         <includes>
            <contentExtension
                  pattern="fr.systerel.explorer.navigator.filters.*">
            </contentExtension>
            <contentExtension
                  isRoot="true"
                  pattern="fr.systerel.explorer.navigator.content.project">
            </contentExtension>
            <contentExtension
                  pattern="fr.systerel.explorer.navigator.content.machineContext">
            </contentExtension>
            <contentExtension
                  pattern="fr.systerel.explorer.navigator.content.invariant">
            </contentExtension>
            <contentExtension
                  pattern="fr.systerel.explorer.navigator.content.event">
            </contentExtension>
            <contentExtension
                  pattern="fr.systerel.explorer.navigator.content.variable">
            </contentExtension>
            <contentExtension
                  pattern="fr.systerel.explorer.navigator.content.theorem">
            </contentExtension>
            <contentExtension
                  pattern="fr.systerel.explorer.navigator.content.axiom">
            </contentExtension>
            <contentExtension
                  pattern="fr.systerel.explorer.navigator.content.constant">
            </contentExtension>
            <contentExtension
                  pattern="fr.systerel.explorer.navigator.content.carrierSet">
            </contentExtension>
            <contentExtension
                  pattern="fr.systerel.explorer.pofiles">
            </contentExtension>
            <contentExtension
                  pattern="fr.systerel.explorer.navigator.content.complex">
            </contentExtension>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resources.workingSets">
            </contentExtension>
         </includes>
      </viewerContentBinding>
      <viewerActionBinding
            viewerId="fr.systerel.explorer.navigator.view">
         <includes>
            <actionExtension
                  pattern="fr.systerel.explorer.navigator.actionProviders.file">
            </actionExtension>
            <actionExtension
                  pattern="fr.systerel.explorer.navigator.actionProviders.project">
            </actionExtension>
            <actionExtension
                  pattern="fr.systerel.explorer.navigator.actionProviders.treeNode">
            </actionExtension>
            <actionExtension
                  pattern="fr.systerel.explorer.navigator.actionProviders.element">
            </actionExtension>
            <actionExtension
                  pattern="fr.systerel.explorer.navigator.actionProviders.po">
            </actionExtension>
         </includes>
      </viewerActionBinding>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <navigatorContent
            contentProvider="org.eventb.internal.ui.RodinProjectContentProvider"
            id="fr.systerel.explorer.navigator.content.project"
            labelProvider="fr.systerel.explorer.navigator.RodinLabelProvider"
            name="%navigatorContent.project"
            priority="normal">
         <possibleChildren>
            <instanceof
                  value="org.rodinp.core.IRodinProject">
            </instanceof>
            <instanceof
                  value="org.eclipse.core.resources.IContainer">
            </instanceof>
         </possibleChildren>
         <triggerPoints>
            <instanceof
                  value="org.rodinp.core.IRodinDB">
            </instanceof>
         </triggerPoints>
      </navigatorContent>
      <navigatorContent
            contentProvider="fr.systerel.explorer.navigator.contentProviders.MachineContextContentProvider"
            id="fr.systerel.explorer.navigator.content.machineContext"
            labelProvider="fr.systerel.explorer.navigator.RodinLabelProvider"
            name="%navigatorContent.machineContexts">
         <triggerPoints>
            <instanceof
                  value="org.rodinp.core.IRodinProject">
            </instanceof>
         </triggerPoints>
         <possibleChildren>
            <or>
               <instanceof
                     value="org.eventb.core.IMachineFile">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IContextFile">
               </instanceof>
            </or>
         </possibleChildren>
         <commonSorter
               class="fr.systerel.explorer.navigator.sorters.MachineContextSorter">
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            contentProvider="fr.systerel.explorer.navigator.contentProviders.InvariantContentProvider"
            id="fr.systerel.explorer.navigator.content.invariant"
            labelProvider="fr.systerel.explorer.navigator.RodinLabelProvider"
            name="%navigatorContent.invariants"
            priority="high">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eventb.core.IMachineFile">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.model.ModelElementNode">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
               <instanceof
                     value="org.eventb.core.IInvariant">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.model.ModelElementNode">
               </instanceof>
            </or>
         </possibleChildren>
         <commonSorter
               class="fr.systerel.explorer.navigator.sorters.AntiSorter">
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            contentProvider="fr.systerel.explorer.navigator.contentProviders.EventContentProvider"
            id="fr.systerel.explorer.navigator.content.event"
            labelProvider="fr.systerel.explorer.navigator.RodinLabelProvider"
            name="%navigatorContent.events"
            priority="lowest">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eventb.core.IMachineFile">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.model.ModelElementNode">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
               <instanceof
                     value="org.eventb.core.IEvent">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.model.ModelElementNode">
               </instanceof></or>
         </possibleChildren>
         <commonSorter
               class="fr.systerel.explorer.navigator.sorters.AntiSorter">
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            contentProvider="fr.systerel.explorer.navigator.contentProviders.VariableContentProvider"
            id="fr.systerel.explorer.navigator.content.variable"
            labelProvider="fr.systerel.explorer.navigator.RodinLabelProvider"
            name="%navigatorContent.variables"
            priority="higher">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eventb.core.IMachineFile">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.model.ModelElementNode">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
               <instanceof
                     value="org.eventb.core.IVariable">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.model.ModelElementNode">
               </instanceof>
            </or>
         </possibleChildren>
         <commonSorter
               class="fr.systerel.explorer.navigator.sorters.AntiSorter">
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            contentProvider="fr.systerel.explorer.navigator.contentProviders.TheoremContentProvider"
            id="fr.systerel.explorer.navigator.content.theorem"
            labelProvider="fr.systerel.explorer.navigator.RodinLabelProvider"
            name="%navigatorContent.theorems"
            priority="normal">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eventb.core.IMachineFile">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IContextFile">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.model.ModelElementNode">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
               <instanceof
                     value="org.eventb.core.ITheorem">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.model.ModelElementNode">
               </instanceof>
            </or>
         </possibleChildren>
         <commonSorter
               class="fr.systerel.explorer.navigator.sorters.AntiSorter">
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            contentProvider="fr.systerel.explorer.navigator.contentProviders.AxiomContentProvider"
            id="fr.systerel.explorer.navigator.content.axiom"
            labelProvider="fr.systerel.explorer.navigator.RodinLabelProvider"
            name="%navigatorContent.axioms"
            priority="normal">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eventb.core.IContextFile">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.model.ModelElementNode">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
               <instanceof
                     value="org.eventb.core.IAxiom">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.model.ModelElementNode">
               </instanceof>
            </or>
         </possibleChildren>
         <commonSorter
               class="fr.systerel.explorer.navigator.sorters.AntiSorter">
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            contentProvider="fr.systerel.explorer.navigator.contentProviders.ConstantsContentProvider"
            id="fr.systerel.explorer.navigator.content.constant"
            labelProvider="fr.systerel.explorer.navigator.RodinLabelProvider"
            name="%navigatorContent.constants"
            priority="high">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eventb.core.IContextFile">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.model.ModelElementNode">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
               <instanceof
                     value="org.eventb.core.IConstant">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.model.ModelElementNode">
               </instanceof>
            </or>
         </possibleChildren>
         <commonSorter
               class="fr.systerel.explorer.navigator.sorters.AntiSorter">
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            contentProvider="fr.systerel.explorer.navigator.contentProviders.CarrierSetContentProvider"
            id="fr.systerel.explorer.navigator.content.carrierSet"
            labelProvider="fr.systerel.explorer.navigator.RodinLabelProvider"
            name="%navigatorContent.carrierSets"
            priority="higher">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eventb.core.IContextFile">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.model.ModelElementNode">
               </instanceof></or>
         </triggerPoints>
         <possibleChildren>
            <or>
               <instanceof
                     value="org.eventb.core.ICarrierSet">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.model.ModelElementNode">
               </instanceof>
            </or>
         </possibleChildren>
         <commonSorter
               class="fr.systerel.explorer.navigator.sorters.AntiSorter">
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            contentProvider="fr.systerel.explorer.navigator.contentProviders.POContentProvider"
            id="fr.systerel.explorer.pofiles"
            labelProvider="fr.systerel.explorer.navigator.RodinLabelProvider"
            name="%navigatorContent.poFiles"
            priority="lowest">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eventb.core.IInvariant">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IAxiom">
               </instanceof>
               <instanceof
                     value="org.eventb.core.ITheorem">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IEvent">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.model.ModelMachine">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.model.ModelContext">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IMachineFile">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IContextFile">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <instanceof
                  value="org.eventb.core.IPSStatus">
            </instanceof>
            <instanceof
                  value="fr.systerel.explorer.model.ModelContext">
            </instanceof>
            <instanceof
                  value="fr.systerel.explorer.model.ModelMachine">
            </instanceof>
         </possibleChildren>
         <commonSorter
               class="fr.systerel.explorer.navigator.sorters.AntiSorter">
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            contentProvider="fr.systerel.explorer.navigator.contentProviders.complex.ComplexContentProvider"
            id="fr.systerel.explorer.navigator.content.complex"
            labelProvider="fr.systerel.explorer.navigator.RodinLabelProvider"
            name="%navigatorContent.complex"
            priority="highest">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.rodinp.core.IRodinProject">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IContextFile">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IMachineFile">
               </instanceof>
            </or>
         </triggerPoints>
         <commonSorter
               class="fr.systerel.explorer.navigator.sorters.AntiSorter">
         </commonSorter>
         <possibleChildren>
            <or>
               <instanceof
                     value="org.eventb.core.IMachineFile">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IContextFile">
               </instanceof>
            </or>
         </possibleChildren>
      </navigatorContent>
      <actionProvider
            class="fr.systerel.explorer.navigator.actionProviders.ProjectActionProvider"
            id="fr.systerel.explorer.navigator.actionProviders.project">
         <enablement>
            <instanceof
                  value="org.rodinp.core.IRodinProject">
            </instanceof>
         </enablement>
      </actionProvider>
      <actionProvider
            class="fr.systerel.explorer.navigator.actionProviders.TreeNodeActionProvider"
            id="fr.systerel.explorer.navigator.actionProviders.treeNode">
         <enablement>
            <or>
               <instanceof
                     value="fr.systerel.explorer.model.ModelMachine">
               </instanceof>
               <instanceof
                     value="org.eventb.ui.projectexplorer.TreeNode">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.model.ModelContext">
               </instanceof>
            </or>
         </enablement>
      </actionProvider>
      <actionProvider
            class="fr.systerel.explorer.navigator.actionProviders.FileActionProvider"
            id="fr.systerel.explorer.navigator.actionProviders.file">
         <enablement>
            <or>
               <instanceof
                     value="org.eventb.core.IMachineFile">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IContextFile">
               </instanceof>
            </or>
         </enablement>
      </actionProvider>
      <actionProvider
            class="fr.systerel.explorer.navigator.actionProviders.ElementActionProvider"
            id="fr.systerel.explorer.navigator.actionProviders.element">
         <enablement>
            <or>
               <instanceof
                     value="org.eventb.core.IAxiom">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IInvariant">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IEvent">
               </instanceof>
               <instanceof
                     value="org.eventb.core.ICarrierSet">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IConstant">
               </instanceof>
               <instanceof
                     value="org.eventb.core.ITheorem">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IVariable">
               </instanceof>
            </or>
         </enablement>
      </actionProvider>
      <actionProvider
            class="fr.systerel.explorer.navigator.actionProviders.POActionProvider"
            id="fr.systerel.explorer.navigator.actionProviders.po">
         <enablement>
            <instanceof
                  value="org.eventb.core.IPSStatus">
            </instanceof>
         </enablement>
      </actionProvider>
      <commonFilter
            activeByDefault="true"
            class="fr.systerel.explorer.navigator.filters.DischargedFilter"
            id="fr.systerel.explorer.navigator.filters.discharged"
            name="%filters.discharged">
      </commonFilter>
      <commonFilter
            activeByDefault="true"
            class="fr.systerel.explorer.navigator.filters.ObligationTextFilter"
            id="fr.systerel.explorer.navigator.filters.obligationText"
            name="%filters.obligationText">
      </commonFilter>
   </extension>
   <extension
         point="org.eclipse.ui.viewActions">
      <viewContribution
            id="fr.systerel.explorer.navigator.viewActions.toolbar"
            targetID="fr.systerel.explorer.navigator.view">
         <action
               class="fr.systerel.explorer.navigator.actionProviders.NewProjectActionDelegate"
               icon="icons/full/clcl16/newprj_wiz.gif"
               id="fr.systerel.explorer.navigator.actions.newProject"
               label="%viewActions.newProject.label"
               toolbarPath="new"
               tooltip="%viewActions.newProject.tooltip">
         </action>
         <action
               class="fr.systerel.explorer.navigator.actionProviders.NewComponentActionDelegate"
               icon="icons/full/clcl16/newcomp_wiz.gif"
               id="fr.systerel.explorer.navigator.actions.newComponent"
               label="%viewActions.newComponent.label"
               toolbarPath="new"
               tooltip="%viewActions.newComponent.tooltip">
         </action>
      </viewContribution>
   </extension>

</plugin>
