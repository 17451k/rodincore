<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.core.runtime.contentTypes">
      <content-type
            base-type="org.rodinp.core.rodin"
            file-extensions="bum"
            id="machine"
            name="%eventBMachine"
            priority="normal"/>
      <content-type
            base-type="org.rodinp.core.rodin"
            file-extensions="buc"
            id="context"
            name="%eventBContext"
            priority="normal"/>
   </extension>
   <extension
         point="org.rodinp.core.fileElementTypes">
      <fileElementType
            class="org.eventb.core.Machine"
            content-type-id="org.eventb.core.machine"
            id="machine"
            name="%eventBMachine"/>
      <fileElementType
            class="org.eventb.core.Context"
            content-type-id="org.eventb.core.context"
            id="context"
            name="%eventBContext"/>
   </extension>
   <extension
         point="org.rodinp.core.internalElementTypes">
      <internalElementType
            class="org.eventb.core.CarrierSet"
            id="carrierSet"
            name="%eventBCarrierSet"
            named="true"/>
      <internalElementType
            class="org.eventb.core.Constant"
            id="constant"
            name="%eventBConstant"
            named="true"/>
      <internalElementType
            class="org.eventb.core.Axiom"
            id="axiom"
            name="%eventBAxiom"
            named="true"/>
      <internalElementType
            class="org.eventb.core.Theorem"
            id="theorem"
            name="%eventBTheorem"
            named="true"/>
      <internalElementType
            class="org.eventb.core.Variable"
            id="variable"
            name="%eventBVariable"
            named="true"/>
      <internalElementType
            class="org.eventb.core.Invariant"
            id="invariant"
            name="%eventBInvariant"
            named="true"/>
      <internalElementType
            class="org.eventb.core.Event"
            id="event"
            name="%eventBEvent"
            named="true"/>
      <internalElementType
            class="org.eventb.core.Guard"
            id="guard"
            name="%eventBGuard"
            named="true"/>
      <internalElementType
            class="org.eventb.core.Action"
            id="action"
            name="%eventBAction"
            
            
            named="true"/>
   </extension>

</plugin>
