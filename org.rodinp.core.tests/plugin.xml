            <renameElement
                  newId="org.rodinp.core.tests.versionEC">
            </renameElement>
            <addAttribute
                  newId="org.rodinp.core.tests.versionStrAttr"
                  newValue="B">
            </addAttribute>
         </element>
         <element
               elementPath="/org.rodinp.core.tests.versionFileD/org.rodinp.core.tests.versionEC">
            <addAttribute
                  newId="org.rodinp.core.tests.versionStrAttr"
                  newValue="C">
