<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="masterDetails" name="%point.masterDetails" schema="schema/masterDetails.exsd"/>
   <extension
         id="org.eclipse.ui.navigator.resourceContent"
         point="org.eclipse.ui.views">
      <view
            category="org.eventb.ui"
            class="fr.systerel.explorer.RodinNavigator"
            icon="icons/full/clcl16/prj_expl.gif"
            id="fr.systerel.explorer.navigator.view"
            name="%view.navigator">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewer
            viewerId="fr.systerel.explorer.navigator.view">
         <options>
            <property
                  name="org.eclipse.ui.navigator.hideLinkWithEditorAction"
                  value="true">
            </property>
         </options>
      </viewer>
      <viewerContentBinding
            viewerId="fr.systerel.explorer.navigator.view">
         <includes>
            <contentExtension
                  pattern="fr.systerel.explorer.navigator.filters.*">
            </contentExtension>
            <contentExtension
                  pattern="fr.systerel.explorer.navigator.content.machineContext">
            </contentExtension>
            <contentExtension
                  pattern="fr.systerel.explorer.navigator.content.invariant">
            </contentExtension>
            <contentExtension
                  pattern="fr.systerel.explorer.navigator.content.event">
            </contentExtension>
            <contentExtension
                  pattern="fr.systerel.explorer.navigator.content.variable">
            </contentExtension>
            <contentExtension
                  pattern="fr.systerel.explorer.navigator.content.theorem">
            </contentExtension>
            <contentExtension
                  pattern="fr.systerel.explorer.navigator.content.axiom">
            </contentExtension>
            <contentExtension
                  pattern="fr.systerel.explorer.navigator.content.constant">
            </contentExtension>
            <contentExtension
                  pattern="fr.systerel.explorer.navigator.content.carrierSet">
            </contentExtension>
            <contentExtension
                  pattern="fr.systerel.explorer.pofiles">
            </contentExtension>
            <contentExtension
                  pattern="fr.systerel.explorer.navigator.content.machine">
            </contentExtension>
            <contentExtension
                  pattern="fr.systerel.explorer.navigator.content.context">
            </contentExtension>
            <contentExtension
                  pattern="fr.systerel.explorer.navigator.content.complex.machine">
            </contentExtension>
            <contentExtension
                  pattern="fr.systerel.explorer.navigator.content.complex.context">
            </contentExtension>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resourceContent">
            </contentExtension>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resources.filters.*">
            </contentExtension>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resources.workingSets">
            </contentExtension>
         </includes>
      </viewerContentBinding>
      <viewerActionBinding
            viewerId="fr.systerel.explorer.navigator.view">
         <includes>
            <actionExtension
                  pattern="fr.systerel.explorer.navigator.actionProviders.file">
            </actionExtension>
            <actionExtension
                  pattern="fr.systerel.explorer.navigator.actionProviders.element">
            </actionExtension>
            <actionExtension
                  pattern="fr.systerel.explorer.navigator.actionProviders.po">
            </actionExtension>
            <actionExtension
                  pattern="org.eclipse.ui.navigator.resources.OpenActions">
            </actionExtension>
            <actionExtension
                  pattern="org.eclipse.ui.navigator.resources.PortingActions">
            </actionExtension>
            <actionExtension
                  pattern="org.eclipse.ui.navigator.resources.NewActions">
            </actionExtension>
            <actionExtension
                  pattern="org.eclipse.ui.navigator.resources.PropertiesActionProvider">
            </actionExtension>
            <actionExtension
                  pattern="org.eclipse.ui.navigator.resources.WorkManagementActionProvider">
            </actionExtension>
            <actionExtension
                  pattern="org.eclipse.ui.navigator.resources.ResourceMgmtActions">
            </actionExtension>
            <actionExtension
                  pattern="org.eclipse.ui.navigator.resources.WorkingSetActions">
            </actionExtension>
         </includes>
      </viewerActionBinding>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <navigatorContent
            contentProvider="fr.systerel.explorer.navigator.contentProviders.InvariantContentProvider"
            id="fr.systerel.explorer.navigator.content.invariant"
            labelProvider="fr.systerel.explorer.navigator.contentProviders.RodinLabelProvider"
            name="%navigatorContent.invariants"
            priority="normal">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eventb.core.IMachineFile">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.navigator.IElementNode">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
               <instanceof
                     value="org.eventb.core.IInvariant">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.navigator.IElementNode">
               </instanceof>
            </or>
         </possibleChildren>
         <commonSorter
               class="fr.systerel.explorer.navigator.sorters.AntiSorter">
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            contentProvider="fr.systerel.explorer.navigator.contentProviders.EventContentProvider"
            id="fr.systerel.explorer.navigator.content.event"
            labelProvider="fr.systerel.explorer.navigator.contentProviders.RodinLabelProvider"
            name="%navigatorContent.events"
            priority="lowest">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eventb.core.IMachineFile">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.navigator.IElementNode">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
               <instanceof
                     value="org.eventb.core.IEvent">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.navigator.IElementNode">
               </instanceof></or>
         </possibleChildren>
         <commonSorter
               class="fr.systerel.explorer.navigator.sorters.AntiSorter">
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            contentProvider="fr.systerel.explorer.navigator.contentProviders.VariableContentProvider"
            id="fr.systerel.explorer.navigator.content.variable"
            labelProvider="fr.systerel.explorer.navigator.contentProviders.RodinLabelProvider"
            name="%navigatorContent.variables"
            priority="high">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eventb.core.IMachineFile">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.navigator.IElementNode">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
               <instanceof
                     value="org.eventb.core.IVariable">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.navigator.IElementNode">
               </instanceof>
            </or>
         </possibleChildren>
         <commonSorter
               class="fr.systerel.explorer.navigator.sorters.AntiSorter">
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            contentProvider="fr.systerel.explorer.navigator.contentProviders.TheoremContentProvider"
            id="fr.systerel.explorer.navigator.content.theorem"
            labelProvider="fr.systerel.explorer.navigator.contentProviders.RodinLabelProvider"
            name="%navigatorContent.theorems"
            priority="normal">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eventb.core.IMachineFile">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IContextFile">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.navigator.IElementNode">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
               <instanceof
                     value="org.eventb.core.ITheorem">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.navigator.IElementNode">
               </instanceof>
            </or>
         </possibleChildren>
         <commonSorter
               class="fr.systerel.explorer.navigator.sorters.AntiSorter">
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            contentProvider="fr.systerel.explorer.navigator.contentProviders.AxiomContentProvider"
            id="fr.systerel.explorer.navigator.content.axiom"
            labelProvider="fr.systerel.explorer.navigator.contentProviders.RodinLabelProvider"
            name="%navigatorContent.axioms"
            priority="normal">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eventb.core.IContextFile">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.navigator.IElementNode">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
               <instanceof
                     value="org.eventb.core.IAxiom">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.navigator.IElementNode">
               </instanceof>
            </or>
         </possibleChildren>
         <commonSorter
               class="fr.systerel.explorer.navigator.sorters.AntiSorter">
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            contentProvider="fr.systerel.explorer.navigator.contentProviders.ConstantsContentProvider"
            id="fr.systerel.explorer.navigator.content.constant"
            labelProvider="fr.systerel.explorer.navigator.contentProviders.RodinLabelProvider"
            name="%navigatorContent.constants"
            priority="high">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eventb.core.IContextFile">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.navigator.IElementNode">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
               <instanceof
                     value="org.eventb.core.IConstant">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.navigator.IElementNode">
               </instanceof>
            </or>
         </possibleChildren>
         <commonSorter
               class="fr.systerel.explorer.navigator.sorters.AntiSorter">
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            contentProvider="fr.systerel.explorer.navigator.contentProviders.CarrierSetContentProvider"
            id="fr.systerel.explorer.navigator.content.carrierSet"
            labelProvider="fr.systerel.explorer.navigator.contentProviders.RodinLabelProvider"
            name="%navigatorContent.carrierSets"
            priority="high">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eventb.core.IContextFile">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.navigator.IElementNode">
               </instanceof></or>
         </triggerPoints>
         <possibleChildren>
            <or>
               <instanceof
                     value="org.eventb.core.ICarrierSet">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.navigator.IElementNode">
               </instanceof>
            </or>
         </possibleChildren>
         <commonSorter
               class="fr.systerel.explorer.navigator.sorters.AntiSorter">
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            contentProvider="fr.systerel.explorer.navigator.contentProviders.POContentProvider"
            id="fr.systerel.explorer.pofiles"
            labelProvider="fr.systerel.explorer.navigator.contentProviders.RodinLabelProvider"
            name="%navigatorContent.poFiles"
            priority="lowest">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eventb.core.IInvariant">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IAxiom">
               </instanceof>
               <instanceof
                     value="org.eventb.core.ITheorem">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IEvent">
               </instanceof>
               <instanceof
                     value="fr.systerel.explorer.navigator.IElementNode">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IMachineFile">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IContextFile">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <instanceof
                  value="org.eventb.core.IPSStatus">
            </instanceof>
            <instanceof
                  value="fr.systerel.explorer.navigator.IElementNode">
            </instanceof>
         </possibleChildren>
         <commonSorter
               class="fr.systerel.explorer.navigator.sorters.AntiSorter">
         </commonSorter>
      </navigatorContent>
      <actionProvider
            class="fr.systerel.explorer.navigator.actionProviders.FileActionProvider"
            id="fr.systerel.explorer.navigator.actionProviders.file">
         <enablement>
            <or>
               <instanceof
                     value="org.eventb.core.IMachineFile">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IContextFile">
               </instanceof>
            </or>
         </enablement>
      </actionProvider>
      <actionProvider
            class="fr.systerel.explorer.navigator.actionProviders.ElementActionProvider"
            id="fr.systerel.explorer.navigator.actionProviders.element">
         <enablement>
            <or>
               <instanceof
                     value="org.eventb.core.IAxiom">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IInvariant">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IEvent">
               </instanceof>
               <instanceof
                     value="org.eventb.core.ICarrierSet">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IConstant">
               </instanceof>
               <instanceof
                     value="org.eventb.core.ITheorem">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IVariable">
               </instanceof>
            </or>
         </enablement>
      </actionProvider>
      <actionProvider
            class="fr.systerel.explorer.navigator.actionProviders.POActionProvider"
            id="fr.systerel.explorer.navigator.actionProviders.po">
         <enablement>
            <instanceof
                  value="org.eventb.core.IPSStatus">
            </instanceof>
         </enablement>
      </actionProvider>
      <navigatorContent
            activeByDefault="false"
            contentProvider="fr.systerel.explorer.navigator.contentProviders.MachineContentProvider"
            id="fr.systerel.explorer.navigator.content.machine"
            labelProvider="fr.systerel.explorer.navigator.contentProviders.RodinLabelProvider"
            name="%navigatorContent.machine"
            priority="higher">
         <triggerPoints>
            <or>
               <adapt
                     type="org.eclipse.core.resources.IProject">
               </adapt>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
               <instanceof
                     value="org.eventb.core.IMachineFile">
               </instanceof>
            </or>
         </possibleChildren>
         <commonSorter
               class="fr.systerel.explorer.navigator.sorters.AntiSorter">
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            activeByDefault="false"
            contentProvider="fr.systerel.explorer.navigator.contentProviders.ContextContentProvider"
            id="fr.systerel.explorer.navigator.content.context"
            labelProvider="fr.systerel.explorer.navigator.contentProviders.RodinLabelProvider"
            name="%navigatorContent.context"
            priority="highest">
         <triggerPoints>
            <instanceof
                  value="org.eclipse.core.resources.IProject">
            </instanceof>
         </triggerPoints>
         <possibleChildren>
            <or>
               <instanceof
                     value="org.eventb.core.IMachineFile">
               </instanceof>
            </or>
         </possibleChildren>
         <commonSorter
               class="fr.systerel.explorer.navigator.sorters.AntiSorter">
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            contentProvider="fr.systerel.explorer.navigator.contentProviders.complex.ComplexContextContentProvider"
            id="fr.systerel.explorer.navigator.content.complex.context"
            labelProvider="fr.systerel.explorer.navigator.contentProviders.RodinLabelProvider"
            name="%navigatorContent.complex.context"
            priority="highest">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eclipse.core.resources.IProject">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IMachineFile">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IContextFile">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <instanceof
                  value="org.eventb.core.IContextFile">
            </instanceof>
         </possibleChildren>
         <commonSorter
               class="fr.systerel.explorer.navigator.sorters.AntiSorter">
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            contentProvider="fr.systerel.explorer.navigator.contentProviders.complex.ComplexMachineContentProvider"
            id="fr.systerel.explorer.navigator.content.complex.machine"
            labelProvider="fr.systerel.explorer.navigator.contentProviders.RodinLabelProvider"
            name="%navigatorContent.complex.machine"
            priority="higher">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eclipse.core.resources.IProject">
               </instanceof>
               <instanceof
                     value="org.eventb.core.IMachineFile">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <instanceof
                  value="org.eventb.core.IMachineFile">
            </instanceof>
         </possibleChildren>
         <commonSorter
               class="fr.systerel.explorer.navigator.sorters.AntiSorter">
         </commonSorter>
      </navigatorContent>
      <commonFilter
            activeByDefault="true"
            class="fr.systerel.explorer.navigator.filters.HideNoneRodinFilter"
            id="fr.systerel.explorer.navigator.filters.hideNonRodin"
            name="%filters.hideNonRodin">
      </commonFilter>
      <commonFilter
            activeByDefault="true"
            class="fr.systerel.explorer.navigator.filters.HideAllResourcesFilter"
            id="fr.systerel.explorer.navigator.filters.hideResources"
            name="%filters.hideResources">
      </commonFilter>
      <commonFilter
            class="fr.systerel.explorer.navigator.filters.BccFilter"
            id="fr.systerel.explorer.navigator.filters.hideBcc"
            name="%filters.hideBcc">
      </commonFilter>
      <commonFilter
            class="fr.systerel.explorer.navigator.filters.BcmFilter"
            id="fr.systerel.explorer.navigator.filters.hideBcm"
            name="%filters.hideBcm">
      </commonFilter>
      <commonFilter
            class="fr.systerel.explorer.navigator.filters.BpoFilter"
            id="fr.systerel.explorer.navigator.filters.hideBpo"
            name="%filters.hideBpo">
      </commonFilter>
      <commonFilter
            class="fr.systerel.explorer.navigator.filters.BprFilter"
            id="fr.systerel.explorer.navigator.filters.hideBpr"
            name="%filters.hideBpr">
      </commonFilter>
      <commonFilter
            class="fr.systerel.explorer.navigator.filters.BpsFilter"
            id="fr.systerel.explorer.navigator.filters.hideBps"
            name="%filters.hideBps">
      </commonFilter>
      <commonFilter
            class="fr.systerel.explorer.navigator.filters.BucFilter"
            id="fr.systerel.explorer.navigator.filters.hideBuc"
            name="%filters.hideBuc">
      </commonFilter>
      <commonFilter
            class="fr.systerel.explorer.navigator.filters.BumFilter"
            id="fr.systerel.explorer.navigator.filters.hideBum"
            name="%filters.hideBum">
      </commonFilter>
      <commonFilter
            activeByDefault="true"
            class="fr.systerel.explorer.navigator.filters.ObligationTextFilter"
            id="fr.systerel.explorer.navigator.filters.obligationText"
            name="%filters.default">
      </commonFilter>
      <commonFilter
            activeByDefault="true"
            class="fr.systerel.explorer.navigator.filters.DischargedFilter"
            id="fr.systerel.explorer.navigator.filters.discharged"
            name="%filters.default">
      </commonFilter>
      <commonFilter
            activeByDefault="true"
            class="fr.systerel.explorer.navigator.filters.ContextFilter"
            id="fr.systerel.explorer.navigator.filters.contexts"
            name="%filters.default">
      </commonFilter>
   </extension>
   <extension
         point="org.eclipse.ui.viewActions">
      <viewContribution
            id="fr.systerel.explorer.navigator.viewActions.toolbar"
            targetID="fr.systerel.explorer.navigator.view">
         <action
               class="fr.systerel.explorer.navigator.actionProviders.NewProjectActionDelegate"
               icon="icons/full/clcl16/newprj_wiz.gif"
               id="fr.systerel.explorer.navigator.actions.newProject"
               label="%viewActions.newProject.label"
               toolbarPath="new"
               tooltip="%viewActions.newProject.tooltip">
         </action>
         <action
               class="fr.systerel.explorer.navigator.actionProviders.NewComponentActionDelegate"
               icon="icons/full/clcl16/newcomp_wiz.gif"
               id="fr.systerel.explorer.navigator.actions.newComponent"
               label="%viewActions.newComponent.label"
               toolbarPath="new"
               tooltip="%viewActions.newComponent.tooltip">
         </action>
      </viewContribution>
   </extension>
   <extension
         point="fr.systerel.explorer.masterDetails">
      <detailsTab
            Class="fr.systerel.explorer.masterDetails.statistics.StatisticsTab"
            id="fr.systerel.explorer.masterDetails.statistics">
      </detailsTab>
   </extension>

</plugin>
